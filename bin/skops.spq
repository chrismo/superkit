op skops_single_line(): (
  over examples
  | yield f"{i} => {o}"
)

op skops_multi_line(): (
  // ¶ is a newline placeholder. § is a NUL placeholder. zq won't output inlined
  // newlines correctly (but super will with `-i line`, and neither tool can
  // parse an inline NUL.
  yield f"{name} [{type}]¶¶{(over examples
                    | yield f'  {i} => {o}'
                    | collect(this)
                    | join(this, '¶') )}§"
)
